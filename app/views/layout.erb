<!DOCTYPE html>
<html>
  <head>
    <title>Bookmark Manager</title>
  </head>

  <body>
    <% if current_user %>
      Welcome <%= current_user.email %>
    <% end %>
  </body>
  <body>
    <div id='password_error'>
      <% if flash[:password_error] %>
        <%= flash[:password_error] %>
      <% end %>
    </div>
  </body>
  <%= yield %>
</html>
